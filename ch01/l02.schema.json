{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://tour.json-schema.org/content/01-Getting-Started/02-Nesting-Objects",
  "type": "object",
  "properties": {
    "name": {
      "type": "object",
      "properties": {
        "firstName": { "type": "string" },
        "lastName": { "type": "string" },
        "middleName": { "type": "string" }
      }
    },
    "age": {
      "type": "integer"
    }
  }
}
