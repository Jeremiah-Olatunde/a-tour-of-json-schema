{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://tour.json-schema.org/content/01-Getting-Started/05-Arrays",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "age": {
      "type": "integer"
    },
    "hobbies": {
      "type": "array",
      "items": { "type": "string" }
    }
  },
  "required": ["name", "age", "hobbies"]
}
