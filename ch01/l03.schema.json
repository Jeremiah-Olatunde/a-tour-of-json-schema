{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://tour.json-schema.org/content/01-Getting-Started/03-Required-Properties",
  "type": "object",
  "properties": {
    "name": {
      "type": "object",
      "properties": {
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "middleName": {
          "type": "string"
        }
      },
      "required": ["firstName", "lastName"]
    },
    "age": {
      "type": "integer"
    }
  }
}
